<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client">
           
           
           <m:MaterialContainer ui:field="x">
    <m:MaterialRow addStyleNames="code">
            <m:MaterialTitle title="Consumer Contact Details" paddingLeft="550"  />
            <m:MaterialRow paddingLeft="500">
             <m:MaterialButton ui:field="show" text="Show details"  width="45%"/>
             </m:MaterialRow>
            <m:MaterialTextBox ui:field="name" readOnly="true" grid="s12" label="Name" width="40%"></m:MaterialTextBox>
            <m:MaterialTextBox  ui:field="add" readOnly="true"  label="Address" width="40%" grid="s12"></m:MaterialTextBox>
            <m:MaterialTextBox ui:field="mob" readOnly="true" label="mobile no." width="40%" grid="s12"></m:MaterialTextBox>
            <m:MaterialTextBox ui:field="email" readOnly="true" label="Email id"  width="40%" grid="s12"></m:MaterialTextBox>
            <m:MaterialTextBox ui:field="cid" readOnly="true" label="Consumer Id"  width="40%" grid="s12"></m:MaterialTextBox>
            <m:MaterialTextBox ui:field="aadhar" readOnly="true" label="Aadhar"  width="40%" grid="s12"></m:MaterialTextBox> 
                </m:MaterialRow>
     
    </m:MaterialContainer>
</ui:UiBinder>